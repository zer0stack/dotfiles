xterm*font: xft:DejaVu Sans Mono:pixelsize=16
xterm*vt100*geometry: 80x50
xterm*scrollBar: false
xterm*rightScrollBar: true
xterm*cursorBlink: true
xterm*jumpScroll: true

*.background: #fefbf3
*.foreground: #000000

! black
*.color0: #EDEDED
*.color8: #969694
! red
*.color1: #D7005F
*.color9: #D7005F
! green
*.color2: #718C00
*.color10: #718C00
! yellow / orange
*.color3: #D75F00
*.color11: #D75F00
! blue
*.color4: #4271AE
*.color12: #4271AE
! magenta
*.color5: #8959A8
*.color13: #8959A8
! cyan
*.color6: #3E999F
*.color14: #3E999F
! white
*.color7: #F5F5F5
*.color15: #2D2D2C
